{
   "name": "restfull-api-hexagonal-architecture",
   "version": "1",
   "description": "",
   "main": "index.js",
   "scripts": {
      "start": "node dist/src/index",
      "dev": "nodemon --exec ts-node ./src/index.ts",
      "migration:generate": "typeorm-ts-node-commonjs -d data-source.ts migration:generate ./src/migrations/default",
      "migration:run": "typeorm-ts-node-commonjs -d data-source.ts migration:run",
      "build": "rm -rf ./dist && tsc",
      "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js"
   },
   "dependencies": {
      "body-parser": "^1.20.2",
      "cors": "^2.8.5",
      "dotenv": "^16.4.5",
      "express": "^4.19.2",
      "http-status": "^1.7.4",
      "moment": "^2.30.1",
      "mysql2": "^3.9.7",
      "nodemon": "^3.1.0",
      "reflect-metadata": "^0.2.2",
      "ts-node": "^10.9.2",
      "typeorm": "^0.3.20",
      "typescript": "^5.4.5"
   },
   "devDependencies": {
      "@types/body-parser": "^1.19.5",
      "@types/express": "^4.17.21",
      "@types/node": "^20.12.11",
      "tsconfig-paths": "^4.2.0"
   }
}
